{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/products",
      "dest": "/index.js"
    },
    {
      "src": "/user/:email",
      "dest": "/index.js"
    },
    {
      "src": "/user",
      "dest": "/index.js"
    },
    {
      "src": "/admin",
      "dest": "/index.js"
    },
    {
      "src": "/product/:id",
      "dest": "/index.js"
    },
    {
      "src": "/orders",
      "dest": "/index.js"
    },
    {
      "src": "/reviews",
      "dest": "/index.js"
    },
    {
      "src": "/user",
      "dest": "/index.js"
    },
    {
      "src": "/users",
      "dest": "/index.js"
    },
    {
      "src": "/order/:id",
      "dest": "/index.js"
    },
    {
      "src": "/create-payment-intent",
      "dest": "/index.js"
    },
    {
      "src": "/order/:id",
      "dest": "/index.js"
    },
    {
      "src": "/product",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
